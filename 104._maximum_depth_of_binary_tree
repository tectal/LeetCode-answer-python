# 时间复杂度: O(N)  空间复杂度: O(1)

# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def maxDepth(self, root):
        """
        :type root: TreeNode
        :rtype: int
        """
        return 1+max(map(self.maxDepth,(root.left,root.right))) if root else 0
