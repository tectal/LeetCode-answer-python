# 排除小于0的数，通过字符串进行反转，对比数字是否相等
class Solution:
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x < 0:
            return False
        else:
            if x == int(str(x)[::-1]):
                return True
            else:
                return False
